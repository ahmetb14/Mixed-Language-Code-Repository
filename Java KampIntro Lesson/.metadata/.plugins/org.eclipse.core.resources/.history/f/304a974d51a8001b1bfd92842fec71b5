package oopIntro;

public class Product {

	// Constructor yapýcý bloktur. Özellik tutucu iþ yapýcý class.
	
	int id;
	String name;
	double unitPrice;
	String detail;
	
	public Product() {
		System.out.println("#Ben Çalýþtým!");
	}

	public Product(int id, String name, double unitPrice, String detail) {
		this();
		this.id = id;
		this.name = name;
		this.unitPrice = unitPrice;
		this.detail = detail;
	}
	
}
